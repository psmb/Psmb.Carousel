prototype(Psmb.Carousel:SlideWrapper) < prototype(Neos.Fusion:Tag) {
    tagName = 'li'
    attributes.class = 'Carousel-item'
    content = ${value}
}

prototype(Psmb.Carousel:Carousel) < prototype(Neos.Fusion:Tag) {
    @process.tmpl = ${'<div data-carousel class="marginTop-single"><div class="frame js_frame">' + value + '</div><span class="js_prev prev"><svg xmlns="http://www.w3.org/2000/svg" width="50" height="50" viewBox="0 0 501.5 501.5"><g><path fill="#2E435A" d="M302.67 90.877l55.77 55.508L254.575 250.75 358.44 355.116l-55.77 55.506L143.56 250.75z"/></g></svg></span><span class="js_next next"><svg xmlns="http://www.w3.org/2000/svg" width="50" height="50" viewBox="0 0 501.5 501.5"><g><path fill="#2E435A" d="M199.33 410.622l-55.77-55.508L247.425 250.75 143.56 146.384l55.77-55.507L358.44 250.75z"/></g></svg></span></div>'}
    tagName = 'ul'
    attributes.class = 'slides js_slides'
    content = ContentCollectionRenderer {
        prototype(ContentCase) {
            @process.wrap = Psmb.Carousel:SlideWrapper
        }
    }
    @process.contentElementWrapping = Neos.Neos:ContentElementWrapping
}
